<h1>this is the <strong>Users</strong> table</h1>
<p><%=link_to 'Create new contact', new_user_path%></p>
<div>
<% @users.each do |user| %>
 <p>id: <%= user.id %></p>
 <p>name: <%= user.name %></p>
 <p>lastname: <%= user.lastname %></p>
 <p>age: <%= user.age %></p>
<p><%= link_to 'Edit', edit_user_path(user), method: :get %></p>
 <p><%= link_to 'Show', user_path(user) %></p>
 <p><%= link_to 'Delete', user_path(user), method: :delete %></p>
 <hr>
<% end %>
</div>
